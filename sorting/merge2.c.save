#include<stdio.h>
void printarray(int*,int);
void merge(int*,int,int*,int);
int main(int argc,char* argv[]){
	int a[]={2,5,9,15};
	int b[]={3,7,30,35,0,0,0,0};
	int lena=sizeof(a)/sizeof(a[0]);
	int lenb=sizeof(b)/sizeof(b[0]);
	printarray(a,lena);
	printarray(b,lenb);
	merge(a,lena,b,lenb);
	printarray(b,lenb);
	return 0;
}
void merge(int*a,int lena, int* b,int lenb){
	int i=lena-1;
	int j=lenb-lena-1;
	int k=lenb-1;
	while(i>=0 && j>=0){
		if(a[i]>=b[j]){
			b[k]=a[i];
			i--;
		}else{
			b[k]=b[j];
			j--;
		}
		k--;
	}
	while(i>=0){
		b[k]=a[i];
		k--;
		i--;
	}
}
void printarray(int*a,int len){
	for(int i=0; i<len; i++){
		printf("%d\t",a[i]);
	}
	printf("\n");
}
